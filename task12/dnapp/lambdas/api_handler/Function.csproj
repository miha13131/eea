		<PackageReference Include="Amazon.Lambda.Serialization.SystemTextJson" Version="2.*" />
		<PackageReference Include="AWSSDK.CognitoIdentityProvider" Version="3.*" />
		<PackageReference Include="AWSSDK.DynamoDBv2" Version="3.*" />
	</ItemGroup>


</Project>
